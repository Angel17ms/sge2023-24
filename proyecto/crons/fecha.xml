<odoo>
    <data>
        <record model="ir.cron" forcecreate="True" id="proyecto.cron_update">
            <field name="name">Cron Update</field>
            <field name="model_id" ref="res.user"/>
            <field name="state">code</field>
            <field name="code">proyecte._compute_days_to_expiry()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>