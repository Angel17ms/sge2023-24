<odoo>
    <data>

    <record id="view_defense_tree" model="ir.ui.view">
        <field name="name">clash.defense.tree</field>
        <field name="model">clash.defense</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="type"/>
                <field name="damage"/>
                <field name="health"/>
                <field name="village_id"/>
                <field name="level"/>
            </tree>
        </field>
    </record>

    <record id="view_defense_form" model="ir.ui.view">
        <field name="name">clash.defense.form</field>
        <field name="model">clash.defense</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="type"/>
                        <field name="damage"/>
                        <field name="health"/>
                        <field name="village_id"/>
                        <field name="level"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_clash_defense_search" model="ir.ui.view">
    <field name="name">clash.defense.search</field>
    <field name="model">clash.defense</field>
    <field name="arch" type="xml">
        <search>
            <field name="name" string="Defense Name"/>
            <field name="type" string="Defense Type"/>
            <field name="level" string="Defense Level"/>
        </search>
    </field>
</record>


    </data>
</odoo>